cpp = ['/opt/poky/2.7.3/sysroots/x86_64-pokysdk-linux/usr/bin/arm-poky-linux-gnueabi/arm-poky-linux-gnueabi-g++', '-mfpu=neon', '-mfloat-abi=hard', '-mcpu=cortex-a8', '--sysroot=/opt/poky/2.7.3/sysroots/cortexa8hf-neon-poky-linux-gnueabi']
ar = ['/opt/poky/2.7.3/sysroots/x86_64-pokysdk-linux/usr/bin/arm-poky-linux-gnueabi/arm-poky-linux-gnueabi-ar']
